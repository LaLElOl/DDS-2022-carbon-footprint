{{#partial "contenido" }}
<body>
    <main>
        <section>
            <h2 class="contacto title">Alta de Vehículo Particular</h2>

            <form class="formulario">
                <fieldset>
                    <div class="campo">
                        <label>Factor de Emisión</label>
                        <input type="number" placeholder="Factor de Emisión">
                    </div>

                    <div class="campo">
                        <label>Factor de Vehículo</label>
                        <input type="number" placeholder="Factor de Vehiculo">
                    </div>

                    <div class="campo">
                        <label>Tipo de Vehículo</label>
                        <select class="mr-6">
                            <option>Auto</option>
                            <option>Moto</option>
                            <option>Camioneta</option>
                        </select>
                    </div>

                    <div class="enviar">
                        <button class="boton" type="submit">Enviar</button>
                    </div>
                </fieldset>
            </form>

        </section>
    </main>


    <footer class>
        <p>Escribir algo copado aca</p>
    </footer>

</body>
{{/partial}}
{{> base.hbs }}