{{#partial "contenido" }}
<body>
    <main>
        <section>
            <h2 class="contacto title">Alta de Vehículo Público</h2>

            <form class="formulario" method="post">
                <fieldset>
                    <div class="campo">
                        <label>Nombre</label>
                        <input name="nombre" type="text" placeholder="Nombre">
                    </div>

                    <div class="campo">
                        <label>Factor de Emisión</label>
                        <input name="factor" type="number" placeholder="Factor de Emisión">
                    </div>

                    <div class="campo">
                        <label>Tipo de Vehículo</label>
                        <select name="tipo" class="mr-6">
                            <option>Colectivo</option>
                            <option>Subte</option>
                            <option>Tren</option>
                        </select>
                    </div>

                    <div class="enviar">
                        <button class="boton" type="submit">Enviar</button>
                    </div>
                </fieldset>
            </form>

        </section>
    </main>


    <footer class>
    </footer>

</body>
{{/partial}}
{{> base.hbs }}